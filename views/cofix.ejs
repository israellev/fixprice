    <script src="https://npmcdn.com/parse@1.10.1/dist/parse.js"></script>
    <script>
        $(document).ready(function() {
            addData()
        });

        function addData() {
            // make arr of object from cofix elemnts
            let elements = $("[class='place-item yy']");
            let cofix = [];
            createData(cofix, elements);
            saveData(cofix);
        }

        function saveData(cofix) {
            // contact with parseDB
            Parse.initialize("aregfdsgfsdzfgs");
            let serverURL = <%- JSON.stringify(serverURL) %>;
            Parse.serverURL = serverURL;
            let cofix_stores = Parse.Object.extend("cofix_stores");
            //saving all data to DB
            for (let i = 0; i < cofix.length; i++) {
                let store = new cofix_stores();
                let point = new Parse.GeoPoint(cofix[i].geoPoint);
                store.set('geoPoint', point);
                store.set('place_description', cofix[i].place_description);
                store.set('linkPictureStore', cofix[i].linkPictureStore);
                store.set('address', cofix[i].address);
                store.set('opening_hours', cofix[i].opening_hours);
                store.set('goToGoogleMap', cofix[i].goToGoogleMap);
                store.set('goToWaze', cofix[i].goToWaze);
                store.save()
                    .then(data => console.log('save'))
                    .catch(err => console.log(err));
            }
        }

        function createData(arr, elements) {
            for (let i = 0; i < elements.length; i++) {
                let latitude,
                    geoPoint = {},
                    place_description = '',
                    linkPictureStore = '',
                    address = '',
                    opening_hours = '',
                    goToGoogleMap = '',
                    goToWaze = '',
                    obj = {};

                //בלקיחה באמצעות הראשי - חיסרון שיכול להיות שנמצא לא באותו מקיום בכל אלמנט
                // בלקיחה באמצעות האמצעי - יכול להיות שידלג על אחד ויבלבל הכל
                try {
                    latitude = $("[class='place-item yy']")[i].attributes[1].value;
                    latitude = parseFloat(latitude);
                } catch (err) {}
                try {
                    let longitude = $("[class='place-item yy']")[i].attributes[2].value;;
                    longitude = parseFloat(longitude);
                    geoPoint = {
                        latitude,
                        longitude
                    };
                } catch (err) {}
                try {
                    place_description = $("[class='place-item yy']")[i].children[0].children[1].children[0].children[0].children[1].innerText;
                    //not correct:
                    // let b = $("[class='place-description']")[i].innerText;
                } catch (err) {}
                try {
                    linkPictureStore = $("[class='place-thumbnail']")[i].children[0].attributes[0].value;
                    // the same:
                    // linkPictureStore = $("[class='place-item yy']")[i].children[0].children[0].children[0].attributes[0].value;
                } catch (err) {}
                try {
                    let street = $("[class='place-address']")[i].innerText.replace('כתובת:', '').trim();
                    let city = $("[class='place-city']")[i].innerText.replace('עיר:', '').trim();
                    address = (street + ', ' + city).trim();
                } catch (err) {}
                try {
                    opening_hours = $("[class='place-opening-hours']")[i].innerText;
                } catch (err) {}
                try {
                    goToGoogleMap = $("[class='goto-google-map']")[i].children[0].attributes[1].value;
                } catch (err) {}
                try {
                    goToWaze = $("[class='goto-waze']")[i].children[0].attributes[1].value;
                    goToWaze = goToWaze.replace('waze://', 'https://waze.com/ul');
                } catch (err) {}

                obj['geoPoint'] = geoPoint;
                obj['place_description'] = place_description;
                obj['linkPictureStore'] = linkPictureStore;
                obj['address'] = address;
                obj['opening_hours'] = opening_hours;
                obj['goToGoogleMap'] = goToGoogleMap;
                obj['goToWaze'] = goToWaze;
                arr.push(obj);
            }
        }
    </script>
